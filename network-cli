#!/bin/bash

# Simple wrapper for IPSW Symbol Server Network Client
# Usage: ./network-cli --server http://IP:8000 crash.ips

SCRIPT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
SERVER_IP="10.100.102.17"

# If no server specified, use default
if [[ "$*" != *"--server"* ]]; then
    exec python3 "$SCRIPT_DIR/ipsw-cli-network.py" --server "http://$SERVER_IP:8000" "$@"
else
    exec python3 "$SCRIPT_DIR/ipsw-cli-network.py" "$@"
fi 