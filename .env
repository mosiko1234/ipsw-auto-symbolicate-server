# IPSW Symbol Server Configuration
DEPLOYMENT_TYPE=4
USE_S3_OPTIMIZED=y
USE_POSTGRES=y
USE_WEB_UI=y
WEB_UI_PORT=5001
API_PORT=8000
SYMBOL_PORT=3993
S3_ENDPOINT=http://host.docker.internal:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET=ipsw
S3_USE_SSL=false
S3_VERIFY_SSL=false
CACHE_SIZE_GB=100
CLEANUP_AFTER_HOURS=24
MAX_CONCURRENT_DOWNLOADS=3
NGINX_PORT=80
# PostgreSQL Configuration
POSTGRES_DB=symbols
POSTGRES_USER=symbols_user
POSTGRES_PASSWORD=symbols_password
